    "lint-staged": "^10.0.7",
    "parcel": "^2.0.0-nightly.118",
    "prettier": "^1.19.1",
    "sass": "^1.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,html,json}": "prettier --write"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "deepmerge": "^4.2.2",
