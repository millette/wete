    "rehype-remark": "^7.0.0",
    "rehype-sanitize": "^3.0.0",
    "rehype-stringify": "^6.0.1",
    "remark-attr": "^0.9.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "remark-stringify": "^7.0.4",
    "to-vfile": "^6.0.0",
    "unified": "^8.4.2",
    "unist-util-visit": "^2.0.2",
    "vfile": "^4.0.2",
    "vfile-reporter": "^6.0.0"
  }
