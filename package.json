{
  "name": "wete",
  "version": "0.0.0",
  "scripts": {
    "dev": "parcel --log-level info --no-autoinstall index.html",
    "build": "parcel build --log-level info --no-source-maps index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "posttest": "npm run format",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md,html,json}\""
  },
  "author": "Robin Millette <robin@millette.info> (http://robin.millette.info/)",
  "license": "AGPL-3.0",
