{
  "name": "wete",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -fr .parcel-cache dist",
    "purgecss": "purgecss --css dist/style.*.css --content dist/index.html dist/main.*.js -o dist",
    "dev": "parcel --log-level info --no-autoinstall index.html",
    "build": "parcel build --log-level info --no-source-maps index.html",
    "deploy": "parcel build --log-level info --no-source-maps --public-url /wete index.html && gh-pages -d dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "posttest": "npm run format",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md,html,json}\""
  },
  "author": "Robin Millette <robin@millette.info> (http://robin.millette.info/)",
  "license": "AGPL-3.0",
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "parcel": "^2.0.0-nightly.120",
    "prettier": "^1.19.1",
    "sass": "^1.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,html,json}": "prettier --write"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "bulmaswatch": "^0.7.5",
    "deepmerge": "^4.2.2",
    "fastify": "^2.12.0",
    "fastify-cookie": "^3.6.0",
    "fastify-static": "^2.6.0",
    "hast-util-sanitize": "^2.0.1",
    "hast-util-select": "^3.0.1",
    "js-cookie": "^2.2.1",
    "lighterhtml": "^2.0.9",
    "mdast-normalize-headings": "^2.0.0",
    "neverland": "^3.1.7",
    "pell": "^1.0.6",
    "rehype-dom-parse": "^2.0.1",
    "rehype-parse": "^6.0.2",
    "rehype-remark": "^7.0.0",
    "rehype-sanitize": "^3.0.0",
    "rehype-stringify": "^6.0.1",
    "remark-attr": "^0.9.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "remark-stringify": "^7.0.4",
    "to-vfile": "^6.0.0",
    "unified": "^8.4.2",
    "unist-util-select": "^3.0.1",
    "unist-util-visit": "^2.0.2",
    "vfile": "^4.0.2",
    "vfile-reporter": "^6.0.0"
  }
}
